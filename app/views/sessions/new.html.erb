  <div class = "row">

    <div class = "small-6 small-offset-3 columns">
<div id = "log-in-box">
  <h2>Log In</h2>
  
  <% if flash[:alert] %>
  <aside class="notice"><%= flash[:alert]%></aside>
  <%end %>

  <%= form_tag do %>


    	<div class="control-group">
        <b><%= label_tag :email %></b>
        <div class="controls">
          <b><%= text_field_tag :email, params[:email] %></b>
        </div>
      </div>

    	<div class="control-group">
        <b><%= label_tag :password %></b>
        <div class="controls">
          <%= password_field_tag :password%>
        </div>
      </div>

      <div class="actions"><%= submit_tag "Log In" %></div>
    <%= link_to 'New User', new_user_path %></button>

    </div>

  </div>

  <% end %>
</div>

<p>&nbsp;</p>
<p>&nbsp;</p>
<p>&nbsp;</p>
